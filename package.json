    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.0",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "core-js",
      "esbuild",
      "sharp",
      "unrs-resolver"
    ]
  }
}
